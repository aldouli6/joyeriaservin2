      <option value="0">COM_SERVIN2_CONSIGNACIONES_PARA_OPTION_0</option>
      <option value="1">COM_SERVIN2_CONSIGNACIONES_PARA_OPTION_1</option>
    </field>
    <field name="cliente" type="foreignkey" label="COM_SERVIN2_FORM_LBL_CONSIGNACION_CLIENTE" description="COM_SERVIN2_FORM_DESC_CONSIGNACION_CLIENTE" class="inputbox" input_type="list" table="#__servin_clientes2" key_field="id" value_field="nombre" hide_trashed="1" default="0" showon="para:0"/>
